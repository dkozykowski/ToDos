<?xml version="1.0" encoding="utf-8"?>
<merge tools:parentTag="android.widget.FrameLayout"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/importanceButtonInactive"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:visibility="visible"
        android:src="@drawable/ic_round_star_border_24" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/importanceButtonActive"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:tint="@color/md_yellow_700"
        android:visibility="visible"
        android:src="@drawable/ic_round_star_24"/>
</merge>